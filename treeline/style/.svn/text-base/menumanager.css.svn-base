	#mm li.selected {
		background-color: #efcedd;
		border-bottom: 1px solid #d16d9a;
		height: 19px;
	}
	
	#mm li.selected div.move {
		color: #8b8b8b;
	}
	
	#mm li.created {
		color: #CA3B7B;
	}
		#mm li.created table td.status {
			/* background: url('/treeline/img/layout/new-page.gif') no-repeat top left; */
			text-indent: -9999px;
		}
	
	#mm li.over {
		background: transparent url('/treeline/img/menumanager/over.gif');
		border-bottom-style: none;
	}	
	
	#mm li div.move {
		cursor:move;
		font-weight: bold;
		color: #AB0D54;
	}	

	#mm li div.status {
		color: #8B8B8B;
	}

