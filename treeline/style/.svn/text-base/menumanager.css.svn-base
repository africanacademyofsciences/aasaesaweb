/* I really hate IE6 */
div.menu-wrap-wrap {
	float: left;
	border: 1px solid #B7C9CA;
	width: 667px;
	background-color: #F6FAFA;
	color: #607072;
	font-size: 90%;
	font-weight: bold;
}
div.menu-wrap {
	float: left;
	width: 627px;
}
	
ul.page-list table {
	width: 100%;
	direction: rtl;
	padding: 0;
	border: 0;
	border-collapse: collapse;
	margin:0;
}
	ul.page-list table tbody {
	}
	ul.page-list table tr {
	}
	ul.page-list table td {
		padding: 0;
		margin: 0;
		text-align: left;
		height:25px;
	}
		ul.page-list table td.title {
			padding-left: 10px;
		}
			ul.page-list table td.title span.newpage {
				background: url('/treeline/img/layout/new-page-flower.gif') no-repeat 10px 0px;
				padding: 3px 32px 5px 0;
			}
		ul.page-list table td.status {
			width: 150px;
		}
		ul.page-list table td.preview {
			width: 15px;	
			text-indent: -9000em;
		}
			ul.page-list table td.preview a.preview {
				display: block;
				background: url('/treeline/img/icons/blue_preview.gif') no-repeat top left;
				outline: none;
				text-decoration: none;
			}			

ul#mm li.no-pages {
	padding: 4px 10px;
}
	#mm {
