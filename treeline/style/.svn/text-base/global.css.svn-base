/*

HTML TAGS

*/



html{
	font-size: 100%;
}

/* BODY */
body{
	background: #DAE3E3;
	color: #000;
	font: 75%/140% Arial, Helvetica, sans-serif;
	padding: 20px 0;
}

/* A  (LINKS) */	
a{
	color: #0067CB;
	font-weight: normal;
	text-decoration: none;
}

a:visited {
	color: #0067CB;
	text-decoration: none;
}

a:hover {
	color: #CF337B;
	text-decoration: underline;
}

a:active {
	color: #6F0033;
	text-decoration: underline;
}

a.tl_link {
	padding-right:13px;
	margin-right:4px;
	background:url('/img/layout/link-marker.gif') right no-repeat;
}
a.help-small {
	margin-left: 6px;
	margin-top: 3px;
	outline: none;
}
a.tags-jump {
	float:left; 
	display:inline; 
	line-height:0%;
}

/* HEADINGS */

h1, h2, h3, h4, h5, h6{
	font: normal 100% Arial, Helvetica, sans-serif;
	padding: 5px 0;
}

h1{
	font-size: 180%;
}

h2{
	font-size: 200%;
}

h3{
	font-size: 160%;
}

h4, h5, h6{ /* keep the same because h5&h6 are rarely used*/
	font-size: 140%;
}

h2.pagetitle {
	background-color: #CA3B7B;
	color: #fff;
	clear: left;
	float: left;
	font-size: 130%;
	padding-left: 22px;
	padding-right: 13px;
	margin-bottom: 17px;
	width: 700px;
}

/* PARAGRAPHS */
p{
	font-size: 100%;
	padding: .5em 0;
}

/* LISTS */

ul, ol, dl{
	font-size: 100%;
	margin: 1em 0 0;
	padding: 0 0 1em;
}

	li{
		font-size: 100%;
		margin: 0 0 0 2em;
	}
	
	li ul, li ol{
		padding: .25em 0;
	}

ul.submenu {
	list-style: none;
	margin: 0;
	padding: 0;	
}
	ul.submenu li {
		background: transparent url('/treeline/img/layout/arrow-li.gif') no-repeat top left;
		margin: 0;
		padding: 0;
		padding-left: 20px;
		margin-bottom: 10px;
	}
	
dt{
	font-size: 100%;
	font-weight: bold;
}

dd{
	font-size: 100%;
	margin-left: 2em;
}


/* HORIZONTAL RULE */

hr{
	border: 1px solid #CED3E0;
	border-width: 1px 0;
	margin: .5em 0;
}

/*

COMMON CLASSES

*/

ul.arrow {margin:0}
ul.arrow li{
	list-style-image: url('/treeline/img/layout/arrow.gif'); 
	margin-bottom: 5px;
	margin-left: 15px;
	padding-bottom: 1px;
}

/* ACCESSIBILITY/SCREEN-READER */

.hide{
	left: -9999px;
	position: absolute;
	top: -9999px;
}

.locked{
	color: #999;
}

/* pagination */

ul.pagination{
	text-align: center;
	float: left;
	width: 735px;
}

ul.pagination li{
	margin: 0;
	padding: 0;
	border: none;
	display: inline;
	font-size: 90%;
	font-weight: bold !important;
}
	ul.pagination li a { 
		font-weight: bold !important;
	}
	ul.pagination li.page {
		padding: 1px 4px;
	}
	ul.pagination li.inactive a{
		color: #606F72;
	}
	ul.pagination li.selected{
		background-color: #497FBF;
		color: #fff;
	}
		ul.pagination li.selected a{
			background: #F3F3F3;
		}
	ul.pagination li.bookend{
		margin-right: 5px;
	}

